{
  "name": "{{name}}",
  "version": "1.0.0",
  "private": true,
  "description": "{{ description }}",
  "scripts": {
    "start": "cross-env NODE_ENV=development san serve --config san.config.js",
    "build": "cross-env NODE_ENV=production san build --config san.config.js",
    "analyzer": "cross-env NODE_ENV=production san build --config san.config.js --analyze",
    "build:modern": "cross-env NODE_ENV=production san build --config san.config.js --modern"
  },
  "keywords": [
    "San",
    "San CLI"
  ],
  {{#lintHook}}
  "lint-staged": {
        "(scripts|src)/**/*.js": [
            "eslint --fix"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    {{/lintHook}}

  "author": "{{author}}",
  "license": "MIT",
  "devDependencies": {
    {{#lint}}
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "babel-eslint": "^10.1.0",
    "prettier": "^2.0.5",
    "eslint-plugin-babel":"^5.3.0",
    "eslint": "^6.8.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-node": "^5.2.0",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^14.1.0",
    {{/if_eq}}
    {{#if_eq lintConfig "ecomfe"}}
    "@ecomfe/eslint-config": "^3.2.0",
    {{/if_eq}}
    {{/lint}}
    {{#if_eq tplEngine "smarty"}}
    "hulk-mock-server": "^0.1.0",
    {{/if_eq}}
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    {{#lintHook}}
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    {{/lintHook}}
    {{#if_eq cssPreprocessor "less"}}
    "less": "^3.11.1",
    "less-loader": "~5.0.0",
    {{/if_eq}}
    {{#if_eq cssPreprocessor "sass"}}
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    {{/if_eq}}
    {{#if_eq cssPreprocessor "stylus"}}
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.0",
    {{/if_eq}}
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "fecs": "^1.6.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "autoprefixer": "^9.7.5",
    "postcss-url": "^7.2.1",
    "san-cli": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    {{#if_eq demoType "store"}}
    "san-store": "1.1.0",
    "san-update": "^2.1.0",
    {{/if_eq}}
    "san": "^3.10.0"
  }
}
